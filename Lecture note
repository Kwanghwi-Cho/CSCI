public class SimpleExample{
  private int max;  
  public int simpleExample(){
    Scannrt input = new Scanner(System.in);
  }  
  public int sumEvens(int max){
    
    int max = input,nexyInt();
    int sum = 0;
    for (int current = 0; current <= max; current+=2){
      sum += current;
    }
    return sum;
  }    
  public double rectArea(double length, double width){
    double result = length * width
  }
}

